<!-- Use this file to deploy some handlers/chains and services      -->
<!-- Two ways to do this:                                           -->
<!--   java org.apache.axis.client.AdminClient deploy.wsdd          -->
<!--      after the axis server is running                          -->
<!-- or                                                             -->
<!--   java org.apache.axis.utils.Admin client|server deploy.wsdd   -->
<!--      from the same directory that the Axis engine runs         -->

<deployment
    xmlns="http://xml.apache.org/axis/wsdd/"
    xmlns:java="http://xml.apache.org/axis/wsdd/providers/java">

  <!-- Services from AccountServiceImplService WSDL service -->

  <service name="AccountServiceImpl" provider="java:RPC" style="wrapped" use="literal">
      <parameter name="wsdlTargetNamespace" value="http://services.mahendra.com"/>
      <parameter name="wsdlServiceElement" value="AccountServiceImplService"/>
      <parameter name="schemaQualified" value="http://services.mahendra.com,http://models.mahendra.com"/>
      <parameter name="wsdlServicePort" value="AccountServiceImpl"/>
      <parameter name="className" value="com.mahendra.services.AccountServiceImplSoapBindingImpl"/>
      <parameter name="wsdlPortType" value="AccountServiceImpl"/>
      <parameter name="typeMappingVersion" value="1.2"/>
      <operation name="save" qname="operNS:save" xmlns:operNS="http://services.mahendra.com" soapAction="" >
        <parameter qname="pns:ac" xmlns:pns="http://services.mahendra.com" type="tns:Account" xmlns:tns="http://models.mahendra.com"/>
      </operation>
      <operation name="getAccounts" qname="operNS:getAccounts" xmlns:operNS="http://services.mahendra.com" returnQName="retNS:getAccountsReturn" xmlns:retNS="http://services.mahendra.com" returnType="rtns:Account" xmlns:rtns="http://models.mahendra.com" soapAction="" >
      </operation>
      <parameter name="allowedMethods" value="save getAccounts"/>

      <typeMapping
        xmlns:ns="http://models.mahendra.com"
        qname="ns:Account"
        type="java:com.mahendra.models.Account"
        serializer="org.apache.axis.encoding.ser.BeanSerializerFactory"
        deserializer="org.apache.axis.encoding.ser.BeanDeserializerFactory"
        encodingStyle=""
      />
  </service>
</deployment>
